/ {
	aliases {
		i2c2 = &i2c_2;
		i2c3 = &i2c_3;
		i2c5 = &i2c_5;
		i2c6 = &i2c_6;
		i2c8 = &i2c_8;
		spi1 = &spi_1;
		spi3 = &spi_3;
	};

	firmware: firmware {
		android {
			fstab {
				vendor {
					dev = "/dev/block/platform/soc/7824900.sdhci/by-name/custom";
					fsmgr_flags = "wait";
				};
				system {
					compatible = "android,system";
					dev = "/dev/block/platform/soc/7824900.sdhci/by-name/system";
					type = "ext4";
					mnt_flags = "ro,barrier=1,discard";
					fsmgr_flags = "wait";
					status = "ok";
				};
			};
		};
	};
};

&spmi_bus {
	qcom,pm8953@0 {
		qcom,power-on@800 {
		qcom,store-hard-reset-reason;
			qcom,pon_1 {
				qcom,support-reset = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <0>;
				qcom,s2-type = <7>;
			};
		};
		/delete-node/ qcom,pm8953_typec@bf00;
	};
};

&pm8953_gpios {
        gpio@c000 {
			qcom,mode = <1>;
			qcom,pull = <5>;
			qcom,vin-sel = <0>;
			qcom,src-sel = <2>;
			qcom,master-en = <1>;
			qcom,out-strength = <2>;
			status = "okay";
        };
};

&pmi8950_gpios {
        gpio@c100 {
			qcom,mode = <2>;
			qcom,output-type = <0>;
			qcom,invert = <0>;
			qcom,pull = <4>;
			qcom,out-strength = <3>;
			qcom,master-en = <1>;
			status = "okay";
        };
};

